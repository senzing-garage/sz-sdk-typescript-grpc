{
    "$schema": "https://typedoc.org/schema.json",
    "entryPoints": ["./src/index.ts"],
    "out": "docs",
    "hideGenerator": true,
    "plugin": ["typedoc-github-theme"],
    "includeVersion": true,
    "excludeNotDocumented": true,
    "excludeNotDocumentedKinds": ["Method", "Constructor"],
    "navigation": {
        "includeCategories": false,
        "includeGroups": true,
        "includeFolders": false,
        "compactFolders": false,
        "excludeReferences": true
    },
    "sidebarLinks": {
        "Senzing Documentation": "http://docs.senzing.com",
        "v4 Beta": "https://senzing.com/docs/4_beta/"
    },
    "categorizeByGroup": true,
    "groupOrder": ["SzGrpcEnvironment", "SzGrpcClasses", "Flags", "Errors", "Properties", "Methods", "Functions", "*"],
    "sort":[
        "kind",
        "instance-first",
        "alphabetical-ignoring-documents"
    ],
    "kindSortOrder": [
        "Reference",
        "Project",
        "Module",
        "Namespace",
        "Enum",
        "EnumMember",
        "Class",
        "Interface",
        "TypeAlias",
        "Constructor",
        "Method",
        "Property",
        "Variable",
        "Function",
        "Accessor",
        "Parameter",
        "TypeParameter",
        "TypeLiteral",
        "CallSignature",
        "ConstructorSignature",
        "IndexSignature",
        "GetSignature",
        "SetSignature"
    ],
    "defaultCategory": "Environment",
    "outputs": [
        {
            "name": "html",
            "path": "docs",
        },
        {
            "name": "json",
            "path": "docs/sz-sdk-typescript-grpc.json"
        }
    ],
    "intentionallyNotExported": [
        "./src/abstracts/szConfig.ts", 
        "./src/abstracts/szConfigManager.ts,",
        "/src/abstracts/szDiagnostic.ts",
        "/src/abstracts/szEngine.ts",
        "/src/abstracts/szProduct.ts",
    ]
}